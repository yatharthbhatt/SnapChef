{
  "expo": {
    "name": "SnapChef",
    "slug": "SnapChef",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/1753389387616.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.yatharth.snapchef"
    }
  }
}
